void MTouchData::setCSMWindow(CSMWindow *pCSMWindow)
{
    _pCSMWindow = pCSMWindow;
}

inline
CSMWindow *MTouchData::getCSMWindow(void) const
{
    return _pCSMWindow;
}

OSG_END_NAMESPACE
