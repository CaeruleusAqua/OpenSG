	</Field>
	<Field
		name="referenceMatrix"
		type="Matrix"
		cardinality="single"
		visibility="external"
        defaultValue=""
	>
	</Field>
	<Field
		name="transformCenter"
		type="Pnt3f"
		cardinality="single"
		visibility="external"
        defaultValue=""
	>
	</Field>
	<Field
		name="worldDiag"
		type="Vec3f"
		cardinality="single"
		visibility="external"
        defaultValue=""
	>
	</Field>
	<Field
		name="mouseData"
		type="MouseData"
		cardinality="single"
		visibility="internal"
		access="public"
	>
	</Field>

	<Field
		name="reset"
		type="OSGAny"
		cardinality="single"
		visibility="internal"
        defaultValue=""
	>
	</Field>
